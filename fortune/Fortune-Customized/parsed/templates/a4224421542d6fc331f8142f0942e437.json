{"components/footer":"<footer class=\"main-footer\">\n\n  <div class=\"container footer-interactions\">\n    <div class=\"layout-module\">\n      <div class=\"footer-navigation footer-navigation-list-item\">\n        {{> components/navigation-footer}}\n      </div>\n\n      {{#if social_media}}\n        <div class=\"footer-social-media footer-navigation-list-item\">\n          <ul>\n            {{#each social_media}}\n            <li class=\"footer-icons\">\n              {{#compare display_name 'google+' operator='==='}}\n                <a href=\"{{url}}\" class=\"google\">\n                  <i class=\"icon-google\"></i>\n                </a>\n              {{else}}\n                <a href=\"{{url}}\" class=\"{{display_name}}\">\n                  <i class=\"icon-{{display_name}}\"></i>\n                </a>\n              {{/compare}}\n            </li>\n            {{/each}}\n          </ul>\n        </div>\n      {{/if}}\n    </div>\n  </div>\n  <div class=\"layout-module\">\n    <div class=\"footer-content\">\n      {{#if settings.show_newsletter_box}}\n        <div class=\"footer-content-column mailing-list-wrapper\">\n          {{> components/common/mailing-list location=\"footer-module\"}}\n        </div>\n      {{/if}}\n\n      <div class=\"footer-content-column\">\n        {{! TODO: When custom blocks come in to play, allow a custom block to be shown here }}\n        {{#if settings.address}}\n          <address class=\"footer-contact-address\">{{nl2br settings.address}}</address>\n        {{/if}}\n\n        {{#if settings.phone_number.length}}\n          <p class=\"footer-contact-phone\">{{settings.phone_number}}</p>\n        {{/if}}\n      </div>\n    </div>\n\n    <div class=\"footer-quick-links\">\n      {{#if settings.account_creation_enabled}}\n        <ul class=\"footer-account-links\">\n          {{#if customer}}\n            <li>\n              <a href=\"{{urls.account.index}}\">{{lang 'account_general.your_account'}}</a>\n            </li>\n            <li>\n              <a href=\"{{urls.auth.logout}}\">{{lang 'account_general.log_out'}}</a>\n            </li>\n          {{else}}\n            <li>\n              <a href=\"{{urls.auth.login}}\">{{lang 'account_general.log_in'}}</a>\n            </li>\n            <li>\n              <a href=\"{{urls.auth.create_account}}\">{{lang 'account_general.sign_up'}}</a>\n            </li>\n          {{/if}}\n        </ul>\n      {{/if}}\n\n      <ul class=\"footer-page-links\">\n        {{! TODO: Any other static links? }}\n        {{#if settings.gift_certificates_enabled}}\n          <li><a href=\"{{urls.gift_certificate.purchase}}\">{{lang 'gift_certificate.heading'}}</a></li>\n        {{/if}}\n        <li><a href=\"{{urls.sitemap}}\">{{lang 'sitemap.heading'}}</a></li>\n      </ul>\n\n      <ul class=\"footer-payment-icons\">\n        {{#if theme_settings.payment-visa}}\n          <li>\n            <span class=\"payment-icon\"><i class=\"icon-payment-visa\"></i></span>\n          </li>\n        {{/if}}\n\n        {{#if theme_settings.payment-mastercard}}\n          <li>\n            <span class=\"payment-icon\"><i class=\"icon-payment-mastercard\"></i></span>\n          </li>\n        {{/if}}\n\n        {{#if theme_settings.payment-american-express}}\n          <li>\n            <span class=\"payment-icon\"><i class=\"icon-payment-american-express\"></i></span>\n          </li>\n        {{/if}}\n\n        {{#if theme_settings.payment-discover}}\n          <li>\n            <span class=\"payment-icon\"><i class=\"icon-payment-discover\"></i></span>\n          </li>\n        {{/if}}\n\n        {{#if theme_settings.payment-paypal}}\n          <li>\n            <span class=\"payment-icon\"><i class=\"icon-payment-paypal\"></i></span>\n          </li>\n        {{/if}}\n\n        {{#if theme_settings.payment-apple-pay}}\n          <li>\n            <span class=\"payment-icon\"><i class=\"icon-payment-apple-pay\"></i></span>\n          </li>\n        {{/if}}\n      </ul>\n\n      {{#if theme_settings.geotrust-ssl-common-name}}\n        {{> core/geotrust-ssl-seal}}\n      {{/if}}\n    </div>\n  </div>\n\n  <div class=\"layout-module\">\n    <p class=\"copyright\" role=\"contentinfo\">&copy; <script>document.write(new Date().getFullYear())</script> <a href=\"{{settings.base_url}}\">{{settings.store_name}}</a></p>\n    {{#if theme_settings.theme-credits}}\n      <p class=\"footer-attribution\">\n        {{{lang 'footer.theme_attribution'\n          theme_name='Fortune'\n          developed='<a href=\"http://pixelunion.net\" target=\"_blank\">Pixel Union</a>'\n          powered_by='<a href=\"https://www.bigcommerce.com/\" target=\"_blank\" rel=\"nofollow\">BigCommerce</a>'\n        }}}\n      </p>\n    {{/if}}\n  </div>\n</footer>\n","components/navigation-footer":"<nav class=\"main-footer-menu\">\n  <ul class=\"nav-menu navigation-tier\">\n    <li>\n      <a href=\"#header-pane-category\" data-slideout-menu>{{lang 'product.label'}}</a>\n    </li>\n    {{#if shop_by_brand}}\n      <li>\n        <a href=\"#header-pane-brand\" data-slideout-menu>{{lang 'brands.label'}}</a>\n      </li>\n    {{/if}}\n    <li>\n      <a href=\"#header-pane-pages\" data-slideout-menu>{{lang 'page.label'}}</a>\n    </li>\n    <li><a href=\"#modal-search\" class=\"toggle-search\">{{lang 'search.label'}}</a></li>\n    <li><a href=\"#header-cart-pane\" data-slideout-menu>{{lang 'cart.label'}}</a></li>\n  </ul>\n</nav>\n\n","components/common/mailing-list":"<section class=\"mailing-list {{location}}\">\n  <h3 class=\"section-title\">{{lang 'footer.mailing_list'}}</h3>\n\n  <p>{{lang 'footer.mailing_list_blurb'}}</p>\n\n  <form\n    class=\"mailing-list-form\"\n    action=\"{{urls.subscribe.action}}\"\n    method=\"post\"\n    data-validates\n  >\n    <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n    <input type=\"hidden\" name=\"check\" value=\"1\">\n    <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n\n    <div class=\"form-field full-width-field\">\n      <label class=\"form-label\" for=\"nl_email\">\n        <span class=\"form-label-text\">{{lang 'common.email_address'}}</span>\n        <input\n          class=\"form-input mailing-list-email\"\n          id=\"nl_email\"\n          name=\"nl_email\"\n          type=\"email\"\n          {{#if customer}}value=\"{{customer.email}}\"{{/if}}\n        >\n      </label>\n\n      <input\n        class=\"mailing-list-submit form-input\"\n        type=\"submit\"\n        name=\"subscribe\"\n        value=\"&#xe60a;\"\n      >\n    </div>\n  </form>\n\n  {{#if settings.show_newsletter_summary}}\n    <p class=\"newsletter-summary\">{{settings.newsletter_summary}}</p>\n  {{/if}}\n</section>\n","core/geotrust-ssl-seal":"<table\n  class=\"geotrust-ssl-seal-table\"\n  width=\"135\"\n  border=\"0\"\n  cellpadding=\"2\"\n  cellspacing=\"0\"\n  title=\"{{lang 'core.geotrust.click_to_verify'}}\"\n>\n  <tr>\n    <td width=\"135\" align=\"center\" valign=\"top\">\n      <script type=\"text/javascript\" src=\"https://seal.geotrust.com/getgeotrustsslseal?host_name={{theme_settings.geotrust-ssl-common-name}}&amp;size={{theme_settings.geotrust-ssl-seal-size}}&amp;lang={{locale_name}}\"></script><br />\n      <a\n        href=\"http://www.geotrust.com/ssl/\"\n        target=\"_blank\"\n        style=\"color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;\"\n      ></a>\n    </td>\n  </tr>\n</table>\n"}
