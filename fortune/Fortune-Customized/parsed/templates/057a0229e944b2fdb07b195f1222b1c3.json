{"components/navigation-header":"\n<section class=\"navigation-wrap\" data-slideout-wrapper>\n  <ul class=\"navigation-tier navigation-tier-secondary\" id=\"header-pane-category\">\n    {{#each categories}}\n      <li class=\"header-navigation-item{{#if children}} has-dropdown{{/if}}\">\n        <a href=\"{{url}}\">{{name}}</a>\n        {{#if children}}\n          <ul class=\"navigation-tier navigation-tier-tertiary\">\n            {{#unless ../../theme_settings.hide-parent-menu-items}}\n              <li class=\"header-navigation-item\">\n                <a href=\"{{url}}\">{{lang 'common.header.view_all' name=name}}</a>\n              </li>\n            {{/unless}}\n\n            {{#each children}}\n              <li class=\"header-navigation-item{{#if children}} has-dropdown{{/if}}\">\n                <a href=\"{{url}}\">{{name}}</a>\n\n                {{#if children}}\n                  <ul class=\"navigation-tier navigation-tier-quaternary\">\n                    {{#unless ../../../../theme_settings.hide-parent-menu-items}}\n                      <li class=\"header-navigation-item\">\n                        <a href=\"{{url}}\">{{lang 'common.header.view_all' name=name}}</a>\n                      </li>\n                    {{/unless}}\n\n                    {{#each children}}\n                      <li class=\"header-navigation-item\">\n                        <a href=\"{{url}}\">{{name}}</a>\n                      </li>\n                    {{/each}}\n                  </ul>\n                {{/if}}\n              </li>\n            {{/each}}\n          </ul>\n        {{/if}}\n      </li>\n    {{/each}}\n  </ul>\n\n  {{#if shop_by_brand}}\n    <ul class=\"navigation-tier navigation-tier-secondary\" id=\"header-pane-brand\">\n      {{#each (limit shop_by_brand 10)}}\n        <li class=\"header-navigation-item\">\n          <a href=\"{{url}}\">{{name}}</a>\n        </li>\n      {{/each}}\n\n      {{#if shop_by_brand.length '>' 10}}\n        <li class=\"header-navigation-item header-navigation-separation\">\n          <a href=\"{{urls.brands}}\">{{lang 'brands.view_brands'}}</a>\n        </li>\n      {{/if}}\n    </ul>\n  {{/if}}\n\n  {{#if pages}}\n    <ul class=\"navigation-tier navigation-tier-secondary\" id=\"header-pane-pages\">\n      {{#each pages}}\n        <li class=\"header-navigation-item{{#if children}} has-dropdown{{/if}}\">\n          <a href=\"{{url}}\">{{name}}</a>\n          {{#if children}}\n            <ul class=\"navigation-tier navigation-tier-tertiary\">\n              {{#each children}}\n                <li class=\"header-navigation-item{{#if children}} has-dropdown{{/if}}\">\n                  <a href=\"{{url}}\">{{name}}</a>\n\n                  {{#if children}}\n                    <ul class=\"navigation-tier navigation-tier-quaternary\">\n                      {{#unless ../../theme_settings.hide-parent-menu-items}}\n                        <li class=\"header-navigation-item\">\n                          <a href=\"{{url}}\">{{lang 'common.header.view' name=name}}</a>\n                        </li>\n                      {{/unless}}\n\n                      {{#each children}}\n                        <li class=\"header-navigation-item\">\n                          <a href=\"{{url}}\">{{name}}</a>\n                        </li>\n                      {{/each}}\n                    </ul>\n                  {{/if}}\n                </li>\n              {{/each}}\n            </ul>\n          {{/if}}\n        </li>\n      {{/each}}\n    </ul>\n  {{/if}}\n\n  <div class=\"navigation-tier navigation-tier-secondary\" id=\"header-cart-pane\">\n    <div class=\"form-field\">\n      <label class=\"quick-cart-currency form-label\" for=\"quick_cart_currency_change\">\n      <span class=\"quick-cart-change-currency form-label-text\">\n        {{lang 'common.currency_converter'}}\n        {{#if currency_selector.currencies.length '>' 1}}\n          <select class=\"quick-cart-currency-selector currency-switcher\" name=\"quick_cart_currency_change\" id=\"quick_cart_currency_change\">\n            {{#each currency_selector.currencies}}\n              <option value=\"{{{switch_url}}}\" {{#if is_active}}selected{{/if}}>\n                {{name}}\n              </option>\n            {{/each}}\n          </select>\n        {{else}}\n          {{{currency_selector.active_currency_name}}}\n        {{/if}}\n      </span>\n      </label>\n    </div>\n\n    {{> components/cart/quick-cart}}\n  </div>\n</section>\n","components/cart/quick-cart":"<div id=\"quick-cart\" class=\"quick-cart{{#unless cart.items.length}} empty{{/unless}}\" data-count=\"{{cart.quantity}}\">\n  <div data-quick-cart-content>\n    {{> components/cart/quick-cart-content this}}\n  </div>\n\n  <div data-quick-cart-totals>\n    {{> components/cart/quick-cart-totals this}}\n  </div>\n</div>\n","components/cart/quick-cart-content":"<div class=\"quick-cart-content-area\">\n\n  {{#if cart.items.length}}\n\n    {{#each cart.items}}\n      {{> components/cart/quick-cart-item this default_image=../theme_settings.default_image}}\n    {{/each}}\n\n  {{else}}\n\n    <div class=\"empty-cart\">{{lang 'cart.empty'}}</div>\n\n  {{/if}}\n</div>\n","components/cart/quick-cart-totals":"\n{{#if cart.items.length}}\n  <div class=\"quick-cart-footer\">\n\n    <div class=\"quick-cart-subtotal\">\n      <div class=\"label\">{{lang 'cart.subtotal'}}:</div>\n      <div class=\"total\">{{cart.sub_total.formatted}}</div>\n    </div>\n\n    <div class=\"quick-cart-actions\">\n      <a\n        href=\"{{urls.checkout.single_address}}\"\n        class=\"button button-primary button-wide checkout-button\"\n      >\n        {{lang 'cart.checkout'}}\n      </a>\n      {{#if cart.show_multiple_address_shipping}}\n        <a class=\"checkout-multiple\" href=\"{{urls.checkout.multiple_address}}\">\n          {{lang 'cart.checkout_multiple'}}\n        </a>\n      {{/if}}\n      <a\n        href=\"{{urls.cart}}\"\n        class=\"button button-secondary button-wide view-cart-button\"\n      >\n        {{lang 'account_general.view_cart'}}\n      </a>\n    </div>\n\n  </div>\n{{/if}}\n","components/cart/quick-cart-item":"<div class=\"quick-cart-item\" data-id=\"{{id}}\" data-qty=\"{{quantity}}\" data-item-row>\n\n  <div class=\"quick-cart-image-column {{#if type \"===\" \"GiftCertificate\"}}type-gift-certificate{{/if}}\">\n\n    <img class=\"quick-cart-image\" src=\"{{getImage image '70x70' (cdn default_image)}}\" alt=\"{{name}}\">\n\n    {{#if type \"!==\" \"GiftCertificate\"}}\n      <span class=\"quick-cart-quantity-overlay\" data-value=\"{{quantity}}\" aria-live=\"polite\" id=\"quick-cart-qty-{{id}}\">\n        {{quantity}}\n      </span>\n\n      <div class=\"quick-cart-item-actions\">\n\n        <button class=\"quick-cart-item-action\" data-quick-cart-update data-item-id=\"{{id}}\" data-action=\"increase\">\n          <i class=\"icon-increase\" aria-hidden=\"true\"></i> <span class=\"sr-only\">{{lang 'cart.product.increase'}}</span>\n        </button>\n\n        <button class=\"quick-cart-item-action\" data-quick-cart-update data-item-id=\"{{id}}\" data-action=\"decrease\">\n          <i class=\"icon-decrease\" aria-hidden=\"true\"></i> <span class=\"sr-only\">{{lang 'cart.product.decrease'}}</span>\n        </button>\n\n      </div>\n    {{/if}}\n\n  </div>\n\n  <div class=\"quick-cart-content-column {{#if type \"===\" \"GiftCertificate\"}}type-giftcertificate{{/if}}\">\n    <h5 class=\"quick-cart-product-name\">{{name}}</h5>\n    {{lang 'cart.quantity_in_cart' quantity=quantity at_price=price.formatted}}\n  </div>\n\n</div>\n"}
