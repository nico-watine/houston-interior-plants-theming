{"components/header":"\n<header class=\"main-header\">\n  <div class=\"layout-module header-container\">\n    <div class=\"header-branding\">\n      <a class=\"header-logo\" href=\"{{urls.home}}\">\n        {{#if settings.store_logo.image.data}}\n          <img class=\"header-logo-image visible\" src=\"{{getImage settings.store_logo.image 'logo'}}\" alt=\"{{settings.store_logo.image.alt}}\">\n        {{/if}}\n        <span class=\"header-logo-text {{#unless settings.store_logo.image.data}}visible no-image{{/unless}}\">\n          {{settings.store_logo.title}}\n        </span>\n      </a>\n    </div>\n\n    <div class=\"header-navigation\">\n      <button class=\"navigation-toggle-mobile-wrapper\" data-mobile-menu-button>\n        <span class=\"navigation-toggle-mobile\" data-mobile-menu-toggle></span>\n      </button>\n\n      {{> components/header-menu}}\n    </div>\n  </div>\n</header>\n","components/header-menu":"\n<nav class=\"main-header-menu\">\n  <ul class=\"nav-menu navigation-tier navigation-tier-primary\" data-navigation-tier-primary>\n    <li class=\"has-slide-out header-navigation-item navigation-item-primary\">\n      <a class=\"navigation-link-primary\" href=\"#header-pane-category\" data-slideout-menu=\"header-pane-category\">{{lang 'product.label'}}</a>\n    </li>\n    {{#if shop_by_brand}}\n      <li class=\"has-slide-out header-navigation-item navigation-item-primary\">\n        <a class=\"navigation-link-primary\" href=\"#header-pane-brand\" data-slideout-menu=\"header-pane-brand\">{{lang 'brands.label'}}</a>\n      </li>\n    {{/if}}\n    <li class=\"has-slide-out header-navigation-item navigation-item-primary\">\n      <a class=\"navigation-link-primary\" href=\"#header-pane-pages\" data-slideout-menu=\"header-pane-pages\">{{lang 'page.label'}}</a>\n    </li>\n    <li class=\"header-navigation-item navigation-item-primary\">\n      <a class=\"navigation-link-primary toggle-search\" href=\"#modal-search\" class=\"toggle-search\">{{lang 'search.label'}}</a>\n    </li>\n    <li class=\"has-slide-out header-navigation-item navigation-item-primary\">\n      <a class=\"navigation-link-primary\" href=\"#header-cart-pane\" data-slideout-menu=\"header-cart-pane\">{{lang 'cart.label'}}</a>\n    </li>\n  </ul>\n</nav>\n\n"}
