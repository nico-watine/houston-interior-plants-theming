{"components/faceted-search/facets/multi":"<div\n  class=\"\n    faceted-search-filter-list\n    {{#if selected}}is-open{{/if}}\n    {{#unless start_collapsed}}is-open{{/unless}}\n  \">\n\n  <ul\n    data-facet-name=\"{{facet}}\"\n    data-count=\"{{items_count}}\"\n    data-has-more-results=\"{{has_more_results}}\">\n    {{#if items}}\n      {{#each items}}\n        <li class=\"faceted-search-item\">\n          <a href=\"{{url}}\" data-faceted-search-facet rel=\"nofollow\">\n            <span class=\"icon {{#if selected}}icon-checked{{else}}icon-unchecked{{/if}}\"></span>\n            {{title}}\n\n            {{#if ../show_product_counts}}\n              {{#if count}}\n                <span class=\"faceted-search-item-count\">\n                  ({{count}})\n                </span>\n              {{/if}}\n            {{/if}}\n          </a>\n        </li>\n      {{/each}}\n    {{else}}\n      <li class=\"faceted-search-item\">{{lang 'sidebar_filters.empty_facet'}}</li>\n    {{/if}}\n  </ul>\n\n  {{#if show_more_toggle}}\n    <button\n      class=\"faceted-search-more\"\n      role=\"button\"\n      data-additional-facets=\"{{facet}}\">\n      <span>{{lang 'sidebar_filters.show_more'}}</span>\n      <span class=\"hidden\">{{lang 'sidebar_filters.show_less'}}</span>\n    </button>\n  {{/if}}\n</div>"}
