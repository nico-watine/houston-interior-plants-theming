{"components/faceted-search/facets/range":"<form class=\"faceted-search-filter-list {{#if selected}}is-open{{/if}} {{#unless start_collapsed}}is-open{{/unless}}\" method=\"get\" data-faceted-search-range>\n  {{#each current_selected_items}}\n    <input type=\"hidden\" name=\"{{param_name}}\" value=\"{{param_value}}\"/>\n  {{/each}}\n\n  <div class=\"form-field faceted-search-range-min\">\n    <input\n      class=\"form-input\"\n      name=\"min_price\"\n      min=\"0\"\n      type=\"number\"\n      value=\"{{min_price}}\"\n      placeholder=\"{{lang 'sidebar_filters.min_price'}}\"\n      required\n    />\n  </div>\n\n  <div class=\"form-field faceted-search-range-max\">\n    <input\n      class=\"form-input\"\n      name=\"max_price\"\n      min=\"0\"\n      type=\"number\"\n      value=\"{{max_price}}\"\n      placeholder=\"{{lang 'sidebar_filters.max_price'}}\"\n      required\n    />\n  </div>\n\n  <div class=\"form-field form-button\">\n    <input class=\"button button-small button-primary\" type=\"submit\" value=\"{{lang 'sidebar_filters.update'}}\">\n  </div>\n\n</form>\n"}
